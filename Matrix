    let matriz = []
    for (let i = 0; i < 5; i++) {
        matriz[i] = []
        for (let j = 0; j < 5; j++) {
            matriz[i][j] = 0
            if(i == j) matriz[i][j] = 1
        }
    }
    let mostrandoMatriz = ""
    for (let i = 0; i < 5; i++) {
        for (let j = 0; j < 5; j++) {
            mostrandoMatriz += matriz[i][j] + "\t"
        }
        mostrandoMatriz += "\n"
    }
    console.log(mostrandoMatriz)


